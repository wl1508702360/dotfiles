!-- Xft settings --!

Xft.dpi:                      96
Xft.autohint:                 false
Xft.antialias:                false
Xft.hinting:                  true
Xft.lcdfilter:                lcddefault
Xft.hintstyle:                hintslight
Xft.rgba:                     rgb
urxvt.intensityStyles:        false
URxvt.termName:               xterm-256color
URxvt.inputMethod:            fcitx
urxvt.geometry:               120x33
!-- Fonts --!
urxvt.font:                   -xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1,-wenquanyi-wenquanyi bitmap song-medium-r-normal--14-140-75-75-p-80-iso10646-1
urxvt.boldFont:               -xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1,-wenquanyi-wenquanyi bitmap song-medium-r-normal--14-140-75-75-p-80-iso10646-1
!urxvt.font:-xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1,xft:wenquanyi bitmap song:pixelsize=15:antialias=False
!urxvt.boldFont:-xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1,xft:wenquanyi bitmap song:pixelsize=15:antialias=False
urxvt.letterSpace:            0
urxvt.cursorBlink:            0
urxvt.cursorUnderline:        0
urxvt.pointerBlank:           True
urxvt.pointerBlankDelay:      6
! PaperColor Theme
!URxvt.foreground:#4D4D4C
!URxvt.background:#EEEEEE
! black
URxvt.color0:                 #EDEDED
URxvt.color8:                 #969694
! red
URxvt.color1:                 #D7005F
URxvt.color9:                 #D7005F
! green
URxvt.color2:                 #718C00
URxvt.color10:                #718C00
! yellow / orange
URxvt.color3:                 #D75F00
URxvt.color11:                #D75F00
! blue
URxvt.color4:                 #4271AE
URxvt.color12:                #4271AE
! magenta
URxvt.color5:                 #8959A8
URxvt.color13:                #8959A8
! cyan
URxvt.color6:                 #3E999F
URxvt.color14:                #3E999F
! ! white
URxvt.color7:                 #4D4D4C
URxvt.color15:                #F5F5F5
URxvt.foreground:             #C3C3C3
URxvt.background:             #1C1C1C
urxvt.colorBD:                #FFFFFF
urxvt.colorUL:                grey
urxvt.cursorColor:            #F7F7F7
urxvt.troughColor:            #8080F0
urxvt.hightlightColor:        #F0F0F0
urxvt.underlineColor:         grey
urxvt.loginShell:             false
urxvt.meta:                   ignore
urxvt.utmpInhibit:            true
urxvt.depth:                  32
urxvt.scrollBar:              false
urxvt.scrollBar_right:        true
urxvt.scrollBar_floating:     false
urxvt.scrollstyle:            plain
urxvt.secondaryScroll:        true
urxvt.lineSpace:              0
! how many lines you want it to save per session
urxvt.saveLines:              32767
urxvt.scrollColor:            #B0B0B0
! with this set to true, when an app sends a bell to the terminal, awesome hilights the appropriate tag(s) ;)
urxvt.urgentOnBell:           true
! these three lines enable clicking on links to open them ;)
!# if you want to enable tab support, append ,tabbed to the next line
!# then use shift+down arrow to create tabs and shift+{left,right} arrows to switch between them
urxvt.perl-ext-common:        default,clipboard,matcher,-option-popup,-selection-popup,-realine
urxvt.matcher.button:         1
urxvt.urlLauncher:            google-chrome
urxvt.keysym.Shift-Control-V: perl:clipboard:paste
urxvt.iso14755:               False
